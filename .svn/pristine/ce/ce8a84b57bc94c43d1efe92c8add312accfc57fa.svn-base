/*右细线（最后模块无）*/
.box_border_r {position: relative;}
.box_border_r:before {content: " ";border-left: 1rpx solid #ccc;position: absolute;bottom: 0; top: 0; right: 0; width: 1px;}
.box_border_r:last-child:before {display:none;}
/*下细线（最后模块无）*/
.box_border_b {position: relative;}
.box_border_b:before {content: " ";border-bottom: 1rpx solid #ccc;position: absolute;left: 0; top: 0; right: 0; height: 1px;}
.box_border_b:last-child:before {display:none;}

