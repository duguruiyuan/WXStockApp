<!--大咖-个人中心-->
<!--顶部-->
<import src="../templates/tab.wxml"/>
<view class="header">
		<view class="weui-flex container">
    	    <view class="weui-flex__item">
                <i class="icon iconfont icon-left text_font_24"></i>
            </view>
			<view class="weui-flex__item text_center">
			个人中心
			</view>
			<view class="weui-flex__item text_right">
				
			</view>
		</view>
</view>
<view class="Personal_Center_bg">
	<image class="img_90 img_Fillet" src="{{userInfo.user_ico}}"></image>
	<view class="text_font_16"> {{userInfo.user_name}}</view>
	<view class="text_font_14 colyellow"> 大咖币：{{userInfo.coin}}</view>
</view>
<view class="weui-cells weui-cells_after-title margin_bottom">
            <view url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd">领取大咖币，笔记免费看</view>
                <navigator class="weui-cell__ft">
                    <button class="weui-btn mini-btn" type="warn" size="mini">领取</button>
                </navigator>
            </view>         
</view>

<view class="container dk_num box_color">
  		<view class="weui-flex">
            <navigator url="attention/attention" class="weui-flex__item text_center border_r">
              <view>{{userInfo.notice_num}}</view>
              <view class="text_font_12 colgray">我的关注</view>
            </navigator>
            <navigator url="footprint/footprint?userFlag={{userInfo.allow_answer}}" class="weui-flex__item text_center text-center border_r">
              <view>{{userInfo.notice_num}}</view>
              <view class="text_font_12 colgray">我的足迹</view>
            </navigator>
            <navigator url="collection/collention" class="weui-flex__item text_center text-center">
              <view>{{userInfo.collect_note_num+userInfo.collect_question_num}}</view>
              <view class="text_font_12 colgray">我的收藏</view>
            </navigator>
        </view>
  </view>

<view class="weui-cells weui-cells_after-title margin_bottom">
            <navigator url="payExpense/payExpense" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgred box_Fillet5"><i class="icon iconfont icon-xiaofei01 text_font_16"></i></span>消费记录</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
			<navigator url="myNews/myNews" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgviolet box_Fillet5"><i class="icon iconfont icon-xiaoxi text_font_16"></i></span>我的消息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
			<navigator url="share/share" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgyellow box_Fillet5"><i class="icon iconfont icon-fenxiang text_font_16"></i></span>我要分享</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
			<navigator url="aboutUs/aboutUs" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgorange box_Fillet5"><i class="icon iconfont icon-guanyu text_font_16"></i></span>关于大咖问股</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
			<navigator url="handBook/handBook" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgblue box_Fillet5"><i class="icon iconfont icon-yewushouce text_font_16"></i></span>使用手册</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>  
</view>
<view class="weui-cells weui-cells_after-title margin_bottom">
            <navigator url="setup/setup" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__bd"><span class="Personal_Center_ico bgblue_violet box_Fillet5"><i class="icon iconfont icon-xitongshezhi text_font_16"></i></span>系统设置</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>         
</view>
